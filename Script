local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "ç©¶æ¥µç§»å‹•ãƒãƒ– v5 | Discordè¿½åŠ ",
   LoadingTitle = "Loading...",
   LoadingSubtitle = "by Grok - ã‚¯ãƒªãƒ¼ãƒ³ç‰ˆ",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "UltimateMoveHubV5",
      FileName = "Config"
   },
   KeySystem = true,
   KeySettings = {
      Title = "ç©¶æ¥µç§»å‹•ãƒãƒ–",
      Subtitle = "ãƒ‘ã‚¹ãƒ¯ãƒ¼ãƒ‰å…¥åŠ›",
      Note = "jsokkzwa",
      FileName = "UltimateKeyV5",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"jsokkzwa"}
   }
})

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

-- å¤‰æ•°
local wsValue = 200
local jpValue = 150
local flySpeed = 100
local speedEnabled = false
local jumpEnabled = false
local infJumpEnabled = false
local flyEnabled = false
local noclipEnabled = false

local flyConnections = {}
local noclipConnection = nil

-- ã‚­ãƒ£ãƒ©å†ç”Ÿæˆæ™‚ã«å¾©å…ƒ
local function onCharacterAdded(char)
   local hum = char:WaitForChild("Humanoid")
   if speedEnabled then hum.WalkSpeed = wsValue end
   if jumpEnabled then hum.JumpPower = jpValue end
end

player.CharacterAdded:Connect(onCharacterAdded)
if player.Character then onCharacterAdded(player.Character) end

local MainTab = Window:CreateTab("ç§»å‹•å¼·åŒ–", 4483362458)

MainTab:CreateSection("åŸºæœ¬ç§»å‹•")

-- WalkSpeed
local SpeedToggle = MainTab:CreateToggle({
   Name = "ğŸƒ WalkSpeed å¼·åŒ–",
   CurrentValue = false,
   Flag = "SpeedToggle",
   Callback = function(Value)
      speedEnabled = Value
      local hum = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
      if hum then hum.WalkSpeed = Value and wsValue or 16 end
   end,
})

local SpeedSlider = MainTab:CreateSlider({
   Name = "Speed èª¿æ•´",
   Range = {16, 500},
   Increment = 10,
   Suffix = "é€Ÿ",
   CurrentValue = 200,
   Flag = "SpeedSlider",
   Callback = function(Value)
      wsValue = Value
      if speedEnabled then
         local hum = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
         if hum then hum.WalkSpeed = Value end
      end
   end,
})

-- JumpPower
local JumpToggle = MainTab:CreateToggle({
   Name = "ğŸ¦˜ JumpPower å¼·åŒ–",
   CurrentValue = false,
   Flag = "JumpToggle",
   Callback = function(Value)
      jumpEnabled = Value
      local hum = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
      if hum then hum.JumpPower = Value and jpValue or 50 end
   end,
})

local JumpSlider = MainTab:CreateSlider({
   Name = "Jump èª¿æ•´",
   Range = {50, 500},
   Increment = 10,
   Suffix = "é«˜",
   CurrentValue = 150,
   Flag = "JumpSlider",
   Callback = function(Value)
      jpValue = Value
      if jumpEnabled then
         local hum = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
         if hum then hum.JumpPower = Value end
      end
   end,
})

-- Infinite Jump
local InfJumpToggle = MainTab:CreateToggle({
   Name = "âˆ ç„¡é™ã‚¸ãƒ£ãƒ³ãƒ—",
   CurrentValue = false,
   Flag = "InfJumpToggle",
   Callback = function(Value)
      infJumpEnabled = Value
   end,
})

UserInputService.JumpRequest:Connect(function()
   if infJumpEnabled then
      local hum = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
      if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
   end
end)

MainTab:CreateSection("Fly")

local FlySlider = MainTab:CreateSlider({
   Name = "âœˆï¸ Fly é€Ÿåº¦",
   Range = {20, 400},
   Increment = 10,
   Suffix = "é€Ÿ",
   CurrentValue = 100,
   Flag = "FlySlider",
   Callback = function(Value) flySpeed = Value end,
})

local FlyToggle = MainTab:CreateToggle({
   Name = "âœˆï¸ Fly (WASD + Spaceâ†‘/Shiftâ†“)",
   CurrentValue = false,
   Flag = "FlyToggle",
   Callback = function(Value)
      flyEnabled = Value
      local char = player.Character
      if not char then return end
      local root = char:FindFirstChild("HumanoidRootPart")
      local hum = char:FindFirstChildOfClass("Humanoid")
      if not root or not hum then return end

      -- ã‚¯ãƒªãƒ¼ãƒ³ã‚¢ãƒƒãƒ—
      for _, conn in pairs(flyConnections) do conn:Disconnect() end
      flyConnections = {}
      if root:FindFirstChild("BodyVelocity") then root.BodyVelocity:Destroy() end
      if root:FindFirstChild("BodyGyro") then root.BodyGyro:Destroy() end
      hum.PlatformStand = false

      if Value then
         hum.PlatformStand = true
         local bv = Instance.new("BodyVelocity")
         bv.MaxForce = Vector3.new(1e6, 1e6, 1e6)
         bv.Velocity = Vector3.new(0,0,0)
         bv.Parent = root
         local bg = Instance.new("BodyGyro")
         bg.MaxTorque = Vector3.new(1e6, 1e6, 1e6)
         bg.P = 9000
         bg.CFrame = workspace.CurrentCamera.CFrame
         bg.Parent = root

         local keys = {w=false, s=false, a=false, d=false, space=false, lshift=false}
         local yVel = 0

         flyConnections[#flyConnections+1] = UserInputService.InputBegan:Connect(function(inp)
            local k = inp.KeyCode.Name:lower()
            if keys[k] ~= nil then keys[k] = true end
         end)
         flyConnections[#flyConnections+1] = UserInputService.InputEnded:Connect(function(inp)
            local k = inp.KeyCode.Name:lower()
            if keys[k] ~= nil then keys[k] = false end
         end)
         flyConnections[#flyConnections+1] = RunService.Heartbeat:Connect(function()
            if not flyEnabled then return end
            local cam = workspace.CurrentCamera
            local move = Vector3.new()
            if keys.w then move = move + cam.CFrame.LookVector end
            if keys.s then move = move - cam.CFrame.LookVector end
            if keys.a then move = move - cam.CFrame.RightVector end
            if keys.d then move = move + cam.CFrame.RightVector end
            if keys.space then yVel = yVel + 4 end
            if keys.lshift then yVel = yVel - 4 end
            yVel = yVel * 0.85
            move = move.Unit * flySpeed
            bv.Velocity = Vector3.new(move.X, yVel, move.Z)
            bg.CFrame = cam.CFrame
         end)
      end
      Rayfield:Notify({Title="Fly", Content=Value and "ON" or "OFF", Duration=2})
   end,
})

MainTab:CreateSection("Player Teleport")

-- ãƒ—ãƒ¬ã‚¤ãƒ¤ãƒ¼ãƒªã‚¹ãƒˆ
local playerList = {"ãªã—"}
local function updatePlayerList()
   playerList = {"ãªã—"}
   for _, p in pairs(Players:GetPlayers()) do
      if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
         table.insert(playerList, p.Name)
      end
   end
   table.sort(playerList, function(a,b) return a ~= "ãªã—" and (b == "ãªã—" or a < b) end)
end

updatePlayerList()

local PlayerDropdown = MainTab:CreateDropdown({
   Name = "ğŸ‘¥ ãƒ†ãƒ¬ãƒãƒ¼ãƒˆå…ˆãƒ—ãƒ¬ã‚¤ãƒ¤ãƒ¼",
   Options = playerList,
   CurrentOption = {"ãªã—"},
   MultipleOptions = false,
   Flag = "PlayerTPDrop",
   Callback = function(Option)
      -- é¸æŠã®ã¿
   end,
})

-- TPãƒœã‚¿ãƒ³
MainTab:CreateButton({
   Name = "ğŸš€ é¸æŠãƒ—ãƒ¬ã‚¤ãƒ¤ãƒ¼ã«TP",
   Callback = function()
      updatePlayerList()
      PlayerDropdown:Refresh(playerList, true)
      local selected = PlayerDropdown.CurrentOption[1]
      if selected == "ãªã—" then
         Rayfield:Notify({Title="ã‚¨ãƒ©ãƒ¼", Content="ãƒ—ãƒ¬ã‚¤ãƒ¤ãƒ¼é¸æŠã—ã¦ï¼", Duration=3})
         return
      end
      local target = Players:FindFirstChild(selected)
      if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
         local myRoot = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
         if myRoot then
            myRoot.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
            Rayfield:Notify({Title="TPæˆåŠŸ", Content=selected .. " ã«TPï¼", Duration=3})
         end
      else
         Rayfield:Notify({Title="ã‚¨ãƒ©ãƒ¼", Content="å¯¾è±¡ä¸åœ¨", Duration=3})
      end
   end,
})

MainTab:CreateSection("ã™ã‚ŠæŠœã‘ & Discord")

local NoclipToggle = MainTab:CreateToggle({
   Name = "ğŸ‘» Noclip (å£ã™ã‚ŠæŠœã‘)",
   CurrentValue = false,
   Flag = "NoclipToggle",
   Callback = function(Value)
      noclipEnabled = Value
      if noclipConnection then noclipConnection:Disconnect() end
      if Value then
         noclipConnection = RunService.Stepped:Connect(function()
            local char = player.Character
            if char then
               for _, part in pairs(char:GetDescendants()) do
                  if part:IsA("BasePart") and part.CanCollide then
                     part.CanCollide = false
                  end
               end
            end
         end)
      end
      Rayfield:Notify({Title="Noclip", Content=Value and "ON" or "OFF", Duration=2})
   end,
})

