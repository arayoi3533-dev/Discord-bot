local scriptUrl = "https://raw.githubusercontent.com/arayoi3533-dev/Discord-bot/main/main.lua"

local success, err = pcall(function()
    loadstring(game:HttpGet(scriptUrl))()
end)

if not success then
    warn("Script Load Failed:", err)
end
-- Banana Hub v2 | ãƒ‘ã‚¹: jsokkzwa | è¶…ã‚·ãƒ³ãƒ—ãƒ«å¼·åŒ–
local pass = "jsokkzwa"
local UIS = game:GetService("UserInputService")
local Players = game.Players
local player = Players.LocalPlayer

-- ãƒ‘ã‚¹ãƒ¯ãƒ¼ãƒ‰å…¥åŠ›GUIï¼ˆãƒãƒŠãƒŠé¢¨ï¼‰
local sg = Instance.new("ScreenGui", game.CoreGui)
local frame = Instance.new("Frame", sg)
frame.Size = UDim2.new(0, 350, 0, 180)
frame.Position = UDim2.new(0.5, -175, 0.5, -90)
frame.BackgroundColor3 = Color3.fromRGB(255, 220, 0)  -- ãƒãƒŠãƒŠé»„è‰²ï¼
frame.BorderSizePixel = 4
frame.BorderColor3 = Color3.fromRGB(200, 150, 0)

local title = Instance.new("TextLabel", frame)
title.Text = "ğŸŒ Banana Hub v2 ğŸŒ"
title.Size = UDim2.new(1, 0, 0, 60)
title.BackgroundTransparency = 1
title.TextColor3 = Color3.fromRGB(100, 50, 0)
title.Font = Enum.Font.GothamBlack
title.TextSize = 32

local tb = Instance.new("TextBox", frame)
tb.Size = UDim2.new(0.8, 0, 0, 50)
tb.Position = UDim2.new(0.1, 0, 0.4, 0)
tb.PlaceholderText = "ãƒ‘ã‚¹ãƒ¯ãƒ¼ãƒ‰ã‚’å…¥ã‚Œã¦ã­ (jsokkzwa)"
tb.BackgroundColor3 = Color3.new(1,1,1)
tb.TextColor3 = Color3.new(0,0,0)
tb.Font = Enum.Font.Gotham
tb.TextSize = 20

local auth = false
tb.FocusLost:Connect(function(enter)
    if enter and tb.Text == pass then
        sg:Destroy()
        auth = true
    end
end)

repeat wait() until auth

-- ã‚­ãƒ£ãƒ©å¼·åŒ–é©ç”¨
local function apply(char)
    local hum = char:WaitForChild("Humanoid")
    local root = char:WaitForChild("HumanoidRootPart")
    
    hum.WalkSpeed = 200
    hum.JumpPower = 150

    -- ç„¡é™ã‚¸ãƒ£ãƒ³ãƒ—
    UIS.JumpRequest:Connect(function()
        hum:ChangeState(Enum.HumanoidStateType.Jumping)
    end)

    -- ã‚­ãƒ¼æ“ä½œ
    UIS.InputBegan:Connect(function(key)
        if key.KeyCode == Enum.KeyCode.C then
            local enabled = not root:FindFirstChild("BananaNoclip")
            if enabled then
                local conn = game.RunService.Stepped:Connect(function()
                    for _, part in pairs(char:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end)
                conn.Name = "BananaNoclip"
                conn.Parent = root
            else
                if root:FindFirstChild("BananaNoclip") then
                    root.BananaNoclip:Disconnect()
                end
            end
            game.StarterGui:SetCore("SendNotification", {
                Title = "ğŸŒ Banana Noclip";
                Text = enabled and "ONï¼å£ã™ã‚ŠæŠœã‘ãƒãƒŠãƒŠã€œ" or "OFF";
                Duration = 3
            })
        end

        if key.KeyCode == Enum.KeyCode.F then
            setclipboard("https://discord.gg/Ax9yJDR6sV")
            game.StarterGui:SetCore("SendNotification", {
                Title = "ğŸŒ Banana Discord";
                Text = "æ‹›å¾…ãƒªãƒ³ã‚¯ã‚³ãƒ”ãƒ¼ã—ãŸã‚ˆï¼æ¥ã¦ã­ã€œ";
                Duration = 4
            })
        end

        if key.KeyCode == Enum.KeyCode.E then
            game.StarterGui:SetCore("SendNotification", {
                Title = "ğŸŒ ãŠçŸ¥ã‚‰ã›";
                Text = "Flyã¯ä»Šãƒ¡ãƒ³ãƒ†ãƒŠãƒ³ã‚¹ä¸­ã ã‚ˆğŸŒ";
                Duration = 3
            })
        end
    end)
end

-- åˆå›ï¼†ãƒªã‚¹ãƒãƒ¼ãƒ³å¯¾å¿œ
if player.Character then apply(player.Character) end
player.CharacterAdded:Connect(apply)

-- èµ·å‹•é€šçŸ¥ï¼ˆãƒãƒŠãƒŠå…¨é–‹ï¼‰
game.StarterGui:SetCore("SendNotification", {
    Title = "ğŸŒ Banana Hub v2 èµ·å‹•ï¼ğŸŒ";
    Text = "Speed200 + ç„¡é™ã‚¸ãƒ£ãƒ³ãƒ—å¸¸æ™‚ONï¼\nC = Noclip | F = Discordã‚³ãƒ”ãƒ¼\nFlyã¯ãƒ¡ãƒ³ãƒ†ä¸­ã ã‚ˆã€œ";
    Duration = 12
})
