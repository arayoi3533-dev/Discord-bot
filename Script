   Name = "ğŸ“± Discordæ‹›å¾…ã‚³ãƒ”ãƒ¼",
   Callback = function()
      setclipboard("https://discord.gg/Ax9yJDR6sV")
      Rayfield:Notify({
         Title = "ã‚³ãƒ”ãƒ¼å®Œäº†ï¼",
